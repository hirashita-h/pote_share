<div class="card w-50 mt-4">
  <div class="card-body">
    <div class="form-group">
      <h3 class="form-label">Potepan Shareでまだ体験したことのない旅をしよう</h3>
      <%= form_with url: rooms_path, method: :get, local: true do |f| %>
        <%= f.label :address, "エリア", class: "form-label" %>
        <%= f.text_field :address, class: "form-control", :placeholder => '東京' %>
        <%= f.submit'検索', class: "btn btn-primary form mt-2" %>
      <% end %>
    </div>
  </div>
</div>
